{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "fp-order-button",
  "title": "Order Button",
  "description": "Order Button",
  "category": "custom-blocks",
  "icon": "admin-links",
  "keywords": [
    "button",
    "order"
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "view.php",
    "postTypes": [
      "post",
      "product"
    ]
  },
  "supports": {
    "align": false,
    "anchor": true,
    "alignContent": false,
    "color": {
      "text": false,
      "background": false,
      "link": false
    },
    "alignText": true,
    "spacing": {
      "customSpacingSize": false,
      "margin": [
        "bottom"
      ]
    },
    "typography": false,
    "fullHeight": false,
    "mode": false
  },
  "attributes": {
    "backgroundColor": {
      "type": "string",
      "default": "purple"
    }
  },
  "example": {
    "attributes": {
      "data": {
        "link": {
          "title": "Buy now!"
        }
      }
    }
  }
}
